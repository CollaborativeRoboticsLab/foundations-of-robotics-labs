{
    "name": "ros jupyter",
    "dockerFile": "../.binder/Dockerfile",
    "remoteUser": "founder",
    "context": "..",
    "containerEnv": {
        "DISPLAY": "${localEnv:DISPLAY}"
    },
    "runArgs": [
        "--network=host",
        "--device=/dev/dri"
    ],
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/founder/work/${localWorkspaceFolderBasename},type=bind",
    "workspaceFolder": "/home/founder/work/${localWorkspaceFolderBasename}",
    "mounts": [
        "source=/tmp/.X11-unix,target=/tmp/.X11-unix,type=bind",
        "source=${localEnv:HOME}${localEnv:USERPROFILE}/.bash_history,target=/home/founder/.bash_history,type=bind"
    ]
}
